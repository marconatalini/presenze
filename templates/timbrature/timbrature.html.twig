{% extends "base.html.twig" %}

{% block title %}Presenze Europrofili{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('calendar') }}
{% endblock %}

{% block javascripts %}
    {#{{ parent() }}#}
    {{ encore_entry_script_tags('calendar') }}
{% endblock %}

{% block body %}
<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
    {% block body_header %}
        {{ parent() }}
    {% endblock %}

    <div class="mt-2" id='calendar'></div>
    <a id="urlTimbratureJson" href="{{ path('timbrature_JSON', {'codice': codice}) }}"></a>

    <footer class="mt-auto text-white-50">
        <p>Rilevatore presenze by <span class="text-white">Marco Natalini</span></p>
    </footer>
</div>
{% endblock %}



